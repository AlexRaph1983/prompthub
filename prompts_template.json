      "tags": ["контент", "копирайтинг", "маркетинг"],
      "prompt_text": "Создай {тип_контента} на тему {тема} для аудитории {аудитория}. Стиль: {стиль}. Длина: {длина} слов.",
      "variables": [
        {
          "name": "тип_контента",
          "type": "string",
          "required": true,
          "description": "Тип создаваемого контента",
          "example": "статью"
        },
        {
          "name": "тема",
          "type": "string", 
          "required": true,
          "description": "Основная тема контента",
          "example": "искусственный интеллект"
        },
        {
          "name": "аудитория",
          "type": "string",
          "required": true,
          "description": "Целевая аудитория",
          "example": "начинающие разработчики"
        },
        {
          "name": "стиль",
          "type": "string",
          "required": false,
          "description": "Стиль изложения",
          "example": "профессиональный"
        },
        {
          "name": "длина",
          "type": "number",
          "required": false,
          "description": "Целевая длина в словах",
          "example": "500"
        }
      ],
      "usage_instructions": "Подробные инструкции по использованию промпта",
      "examples": [
        "тип_контента: пост в блог; тема: машинное обучение; аудитория: студенты; стиль: доступный; длина: 300"
